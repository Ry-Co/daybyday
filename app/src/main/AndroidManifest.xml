<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="stick.compacto.daybyday">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:roundIcon="@mipmap/logo_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-1943666470588579~3028228331"/>
        <activity
            android:theme="@style/SettingsTheme"
            android:name="stick.compacto.daybyday.ui.main.SettingsActivity"
            android:label="@string/title_activity_settings">
        </activity>
        <activity
            android:name="stick.compacto.daybyday.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="stick.compacto.daybyday.AlarmBroadcastReceiver" />
    </application>

</manifest>